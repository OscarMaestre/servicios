import java.io.IOException;
import java.net.ServerSocket;

public class ServidorCalculo {

	
	public static void main(String[] args) {
		ServerSocket socket;
		int PUERTO=9876
		try {
			socket=new ServerSocket(PUERTO);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
